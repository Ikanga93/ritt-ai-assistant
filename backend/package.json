{
  "name": "multimodal-agent-node",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r menu_data dist/ && cp -r src/templates dist/",
    "lint": "eslint -f unix \"**/*.ts\"",
    "test": "jest",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d dist/database.js",
    "migration:revert": "npm run typeorm -- migration:revert -d dist/database.js",
    "db:backup": "./scripts/backup-db.sh",
    "db:restore": "./scripts/restore-db.sh"
  },
  "packageManager": "pnpm@9.15.9",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^5.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@livekit/agents": "^0.7.0",
    "@livekit/agents-plugin-openai": "^0.9.0",
    "@livekit/rtc-node": "^0.13.3",
    "@sendgrid/mail": "^8.1.5",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.11.13",
    "async-retry": "^1.3.3",
    "bullmq": "^5.49.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "stripe": "^18.0.0",
    "ts-node": "^10.9.2",
    "twilio": "^5.5.2",
    "typeorm": "^0.3.22",
    "typeorm-ts-node-esm": "^0.3.20",
    "zod": "^3.23.8"
  }
}
